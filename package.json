{
  "name": "bizhong.github.io",
  "version": "0.1.0",
  "private": true,
  "description": "LAN Bizhong's personal website",
  "author": "LAN Bizhong <bizhong_lan@163.com> (https://lanbizhong.com/)",
  "scripts": {
    "serve": "vue-cli-service serve --open",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@lbzui/vue": "^0.3.0",
    "core-js": "^3.4.4",
    "register-service-worker": "^1.6.2",
    "vue": "^2.6.10",
    "vue-class-component": "^7.0.2",
    "vue-property-decorator": "^8.3.0",
    "vue-router": "^3.1.3",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.1.0",
    "@vue/cli-plugin-pwa": "^4.1.0",
    "@vue/cli-plugin-router": "^4.1.0",
    "@vue/cli-plugin-typescript": "^4.1.0",
    "@vue/cli-plugin-vuex": "^4.1.0",
    "@vue/cli-service": "^4.1.0",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "normalize.css": "^8.0.1",
    "postcss-preset-env": "^6.7.0",
    "postcss-pxtorem": "^4.0.1",
    "style-resources-loader": "^1.3.2",
    "typescript": "~3.5.3",
    "vue-cli-plugin-style-resources-loader": "^0.1.4",
    "vue-template-compiler": "^2.6.10"
  },
  "postcss": {
    "plugins": {
      "postcss-preset-env": {
        "stage": 3,
        "autoprefixer": {
          "flexbox": "no-2009",
          "grid": "no-autoplace"
        },
        "importFrom": [
          "src/assets/css/lbzui/variables.css"
        ]
      },
      "postcss-pxtorem": {
        "unitPrecision": 8,
        "propList": [
          "font",
          "font-size",
          "line-height"
        ]
      }
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie < 10"
  ],
  "license": "Apache-2.0"
}
